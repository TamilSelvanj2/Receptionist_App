<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardViewUpcoming"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="20dp"
        app:cardCornerRadius="@dimen/_8sdp"
        android:layout_marginBottom="@dimen/_20sdp"
        android:background="#F0F1F3"
        android:layout_gravity="center">

        <RelativeLayout
            android:layout_width="match_parent"
            android:background="@drawable/boder_upcoming"
            android:layout_height="match_parent">

            <LinearLayout
                android:id="@+id/linearUpcomingPatient"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_marginTop="10dp"
                    android:text="APPOINTMENT ID :"
                    android:textColor="@color/black"
                    android:textSize="13sp"
                    android:textStyle="bold" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/textViewAppointmentIdUpcomingPatient"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="@dimen/_20sdp"
                    android:textColor="#07518F"
                    android:textSize="15sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <TextView
                android:id="@+id/underline"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@id/linearUpcomingPatient"
                android:layout_marginStart="@dimen/_20sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:background="@color/black" />

            <LinearLayout
                android:id="@+id/linearLayout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:layout_marginTop="@dimen/_40sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="DOCTORNAME"
                    android:fontFamily="sans-serif-medium"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textSize="@dimen/_10sdp"
                    android:layout_gravity="center"
                    android:textColor="#AEAEAE" />



                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/textViewDoctorNameUpcomingPatient"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Appointment ID"
                    android:layout_weight="1"
                    android:fontFamily="sans-serif-medium"
                    android:textStyle="bold"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:textSize="@dimen/_12sdp"
                    android:textColor="#07518F" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/linearLayout1"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:gravity="start"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SPECIALITY "
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textSize="@dimen/_10sdp"
                    android:fontFamily="sans-serif-medium"
                    android:layout_gravity="center"
                    android:textColor="#AEAEAE" />



                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/textViewSpecialityUpcomingPatient"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Appointment ID"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:fontFamily="sans-serif-medium"
                    android:layout_marginStart="@dimen/_26sdp"
                    android:textSize="@dimen/_12sdp"
                    android:textColor="#494949" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/linearLayout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/linearLayout2"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="PAYMENT"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textSize="@dimen/_10sdp"
                    android:fontFamily="sans-serif-medium"
                    android:layout_gravity="center"
                    android:textColor="#AEAEAE" />



                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/textViewStatusUpcomingPatient"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Appointment ID"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:layout_weight="1"
                    android:fontFamily="sans-serif-medium"
                    android:layout_marginStart="@dimen/_37sdp"
                    android:textSize="@dimen/_12sdp"
                    android:textColor="#07518F" />
            </LinearLayout>
