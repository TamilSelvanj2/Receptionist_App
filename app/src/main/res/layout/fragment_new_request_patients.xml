<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.newRequestPatient.NewRequestPatientsFragment">

    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_8sdp"
        android:layout_marginEnd="@dimen/_8sdp"
        app:cardCornerRadius="@dimen/_8sdp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ProgressBar
                android:id="@+id/progressBarRequestsPatient"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
            <LinearLayout
                android:id="@+id/linearLayout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="PATIENT ID"
                    android:fontFamily="sans-serif-medium"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textSize="@dimen/_11sdp"
                    android:textColor="#AEAEAE"
                    />


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/patientIDsTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-medium"
                    android:text="Patient ID"
                    android:layout_marginStart="@dimen/_50sdp"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"
                    android:textColor="#494949"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_below="@id/linearLayout1"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" NAME"
                    android:fontFamily="sans-serif-medium"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textSize="@dimen/_11sdp"
                    android:textColor="#AEAEAE"
                    />


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/patientNamesTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="PatientName"
                    android:textStyle="bold"
                    android:layout_marginStart="@dimen/_73sdp"
                    android:textSize="@dimen/_11sdp"
                    android:fontFamily="sans-serif-medium"
                    android:textColor="#494949"
                    />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/linearLayout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:layout_below="@id/linearLayout2"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="EMAIL"
                    android:fontFamily="sans-serif-medium"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textSize="@dimen/_11sdp"
                    android:textColor="#AEAEAE"
                    />



                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/patientEmailsTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Patient Name"
                    android:layout_marginStart="@dimen/_74sdp"
                    android:textStyle="bold"
                    android:fontFamily="sans-serif-medium"
                    android:textSize="@dimen/_11sdp"
                    android:textColor="#494949"
                    />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/linearLayout4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:layout_below="@id/linearLayout3"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" NUMBER"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textSize="@dimen/_11sdp"
                    android:fontFamily="sans-serif-medium"
                    android:textColor="#AEAEAE"
                    />


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/patientNumbersTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Patient Number"
                    android:textStyle="bold"
                    android:fontFamily="sans-serif-medium"
                    android:layout_marginStart="@dimen/_59sdp"
                    android:textSize="@dimen/_11sdp"
                    android:textColor="#494949"
                    />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/linearLayout5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:layout_below="@id/linearLayout4"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="GENDER"
                    android:fontFamily="sans-serif-medium"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textSize="@dimen/_11sdp"
                    android:textColor="#AEAEAE"
                    />


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/PatientGendersTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Gender"
                    android:textStyle="bold"
                    android:fontFamily="sans-serif-medium"
                    android:layout_marginStart="@dimen/_65sdp"
                    android:textSize="@dimen/_11sdp"
                    android:textColor="#494949"
                    />

            </LinearLayout>
            <LinearLayout
                android:id="@+id/linearLayout6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:layout_below="@id/linearLayout5"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ADE"
                    android:fontFamily="sans-serif-medium"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:textSize="@dimen/_11sdp"
                    android:textColor="#AEAEAE"
                    />


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/PatientAgesTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="age"
                    android:textStyle="bold"
                    android:fontFamily="sans-serif-medium"
                    android:layout_marginStart="@dimen/_86sdp"
                    android:textSize="@dimen/_11sdp"
                    android:textColor="#494949"
                    />

            </LinearLayout>
        </RelativeLayout>

    </androidx.cardview.widget.CardView>

</FrameLayout>